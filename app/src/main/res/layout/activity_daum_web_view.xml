<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.cityparcelproject.cityparcel.webView.DaumWebViewActivity">

    <WebView
        android:id="@+id/webView_daum"
        android:layout_width="0dp"
        android:layout_height="0dp"
       />

    <TextView
        android:id="@+id/textview_daumPostalCodeResult"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="@string/blank"
        android:visibility="invisible"
        />

    <TextView
        android:id="@+id/textview_daumResult"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0.5"
        android:textSize="20dp"
        android:textColor="@android:color/black"
        android:gravity="center"
        android:text="@string/blank"
        />

    <Button
        android:id="@+id/button_daumWebViewDone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="15dp"
        android:textStyle="bold"
        android:background="@drawable/rounded_rectangle_shape"
        android:text="@string/done"
        />

</LinearLayout>
